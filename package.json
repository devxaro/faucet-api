{
  "name": "flokicoin-faucet-server",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "nodemon",
    "pm2": "pm2-runtime npm -- run start",
    "build": "tsc -p tsconfig.json",
    "build:types": "npm run clean && tsc --emitDeclarationOnly --declaration -p tsconfig.json",
    "tokens": "node ./bin/scripts/tokens_generator.js",
    "reset": "rm -rf dist node_modules log && npm cache clean -f",
    "clean": "rm -rf dist log",
    "pre-commit": "lint-staged",
    "linter": "eslint --ext .ts ./src",
    "linter-fix": "eslint --fix --ext .ts ./src",
    "prettier": "prettier --write ./src",
    "prepare": "husky",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fastify-decorators/typedi": "^4.0.0-next.5",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/swagger": "^8.15.0",
    "axios": "^1.9.0",
    "cron": "^4.3.0",
    "execa": "^9.5.2",
    "fastify": "^4.27.0",
    "fastify-decorators": "^4.0.0-next.5",
    "fastify-plugin": "^4.5.1",
    "fastify-zod": "^1.4.0",
    "flokicoinjs-lib": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "tmp": "^0.2.3",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.1",
    "undici": "6.19.2",
    "url": "^0.11.3",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cspell/eslint-plugin": "^8.11.0",
    "@types/got": "^9.6.12",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitest/coverage-v8": "^2.0.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "got": "^14.4.1",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.9",
    "pm2": "^5.4.1",
    "portfinder": "^1.0.32",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --ext .ts ./src --fix",
      "prettier --write ./src",
      "npm run build"
    ]
  }
}
